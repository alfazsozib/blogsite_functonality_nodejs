<%- include("partials/header") %>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="exampleFormControlTextarea1">Title</label>
        <input class="form-control" name="postTitle" id="exampleFormControlTextarea1">
    </div>
    <div class="form-group">
        <label for="exampleFormControlTextarea1">Post</label>
        <textarea class="form-control" name="postBody" id="exampleFormControlTextarea1" rows="3"></textarea>
    </div>
<button type="submit" class="btn btn-success" name="button">Publish</button>
</form>

<%- include("partials/footer") %>